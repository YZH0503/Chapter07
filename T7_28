#include <iostream>
#include <string>
#include <cstring>
#include <algorithm>
using namespace std;

int main()
{
    string a, b;
    cin >> a;
    b = a;
    reverse( a.begin() , a.end() );
    if(a==b)
    {
        cout << "是回文数" <<endl;
    }
    else
    {
        cout << "不是回文数" <<endl;
    }


    return 0;
}
